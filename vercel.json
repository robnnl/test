{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps",
  "env": {
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 